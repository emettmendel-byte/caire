        "variable": "chest_pain",
        "operator": "==",
        "threshold": true
      },
      "children": ["n2", "n3"],
      "metadata": { "confidence": 0.95 }
    },
    "n2": {
      "id": "n2",
      "type": "action",
      "label": "Chest pain present - urgent evaluation",
      "action": {
        "recommendation": "Proceed to ED or urgent cardiac evaluation.",
        "urgency_level": "urgent"
      },
      "children": [],
      "metadata": { "confidence": 0.9 }
    },
    "n3": {
      "id": "n3",
      "type": "action",
      "label": "No chest pain",
      "action": {
        "recommendation": "Continue with routine triage pathway.",
        "urgency_level": "routine"
      },
      "children": [],
      "metadata": { "confidence": 0.95 }
    }
  },
  "variables": [
    {
      "name": "chest_pain",
      "type": "boolean",
      "source": "patient_history",
      "description": "Patient reports chest pain"
    }
  ]
