{
  "id": "example-triage-v1",
  "name": "Example Emergency Triage",
  "version": "1.0.0",
  "domain": "emergency_triage",
  "root_node_id": "n1",
  "nodes": {
    "n1": {
      "id": "n1",
      "type": "condition",
      "label": "Does the patient have chest pain?",
